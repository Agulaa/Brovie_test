<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:media="clr-namespace:System.Windows.Media;assembly=PresentationCore">
    <media:Brush x:Key="BackgroundBrush">LightGray</media:Brush>
    <media:Brush x:Key="ColorBrush">DodgerBlue</media:Brush>
    <media:Brush x:Key="HighlightBrush">SkyBlue</media:Brush>
    <media:Brush x:Key="FontBrush">White</media:Brush>
</ResourceDictionary>